<?xml version="1.0" encoding="UTF-8"?>
<xp:view xmlns:xp="http://www.ibm.com/xsp/core">
		<xp:panel styleClass="sbo-object-card">
			<xp:link text="#{javascript:peerObject.getObjectTitle()}" id="label3">
				<xp:this.value><![CDATA[#{javascript: return "main.xsp?objectId=" + peerObject.getObjectId() + "&objectName=" + param.get('objectName') + "&objectPeers=" + param.get('objectPeers');}]]></xp:this.value>
			</xp:link>
			<div>
				<xp:panel styleClass="sbo-object-card-img-container">
					<xp:image styleClass="sbo-object-img" url="/placeholder_person.jpg" id="image1"></xp:image>
				</xp:panel>
				<xp:panel styleClass="sbo-object-card-fields-container">
					<xp:repeat id="repeat5" var="field1">
						<xp:this.value>
							<!-- Change to preview attribute list getPreviewAttribteList() -->
							<![CDATA[#{javascript:var map:java.util.HashMap = peerObject.getAttribteList1(); return map.entrySet();}]]>
						</xp:this.value>
						<div class="sbo-keyvaluepair">
							<xp:label value="#{javascript:field1.getKey() + ': '}" id="label1"></xp:label>
							<xp:label value="#{javascript:field1.getValue()}" id="label2"></xp:label>
						</div>
					</xp:repeat>
				</xp:panel>
			</div>
		</xp:panel>
</xp:view>
